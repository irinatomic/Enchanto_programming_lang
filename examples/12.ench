enchanto {

    declaro incantum broj eksponent rezultat;
    declaro stringo ispis;

    revelio "Unesite_broj_za_izračunavanje_logaritma: ";
    scriptum broj;

    eksponent = 0;
    rezultat = 1;

    whilegardium broj > 1 du {
        broj = broj / 2;
        eksponent = eksponent + 1;
    }

    ispis = "Logaritam_osnove_2_od_unetog_broja_je: " + eksponent;
    revelio ispis;
}
