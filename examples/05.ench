enchanto {

    declaro incantum broj a b c;

    revelio "Unesite_pozitivne_cele_brojeve:";
    scriptum broj;

    a = broj - 10;
    b = broj * 10;
    c = broj / 10;

    broj = a * 3 + b * 2 + c;

    mystime broj > 200 du {
        revelio "Broj je lud";
    } returntime du {
        revelio "Broj nije lud";
    }

}